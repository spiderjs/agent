{
    "appenders": [{
            "type": "console"
        },
        {
            "type": "dateFile",
            "filename": "logs/spiderjs-agent.log",
            "pattern": "-yyyy-MM-dd",
            "maxLogSize": 20480,
            "category": "spiderjs-agent"
        },
        {
            "type": "dateFile",
            "filename": "logs/spider-agent-executor.log",
            "pattern": "-yyyy-MM-dd",
            "maxLogSize": 20480,
            "category": "spider-agent-executor"
        },
        {
            "type": "dateFile",
            "filename": "logs/spiderjs-agent-worker.log",
            "pattern": "-yyyy-MM-dd",
            "maxLogSize": 20480,
            "category": "spiderjs-agent-worker"
        }
    ]
}
