{
    "appenders": [
        {
            "type": "logLevelFilter",
            "level": "INFO",
            "appender": {
                "type": "console"
            }
        },
        {
            "type": "logLevelFilter",
            "level": "DEBUG",
            "appender": {
                "type": "dateFile",
                "filename": "logs/agent.log",
                "pattern": "-yyyy-MM-dd",
                "maxLogSize": 20480
            }
        }
    ]
}